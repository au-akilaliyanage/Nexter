{
  "name": "nexter",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "watch": "sass --watch --no-source-map sass/main.scss css/style.css",
    "compile": "sass --no-source-map sass/main.scss css/style.comp.css",
    "prefix": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css ",
    "compress": "sass --no-source-map css/style.prefix.css css/style.css --style compressed",
    "build": "npm-run-all compile prefix compress"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "sass": "^1.92.1"
  }
}
